{
  "name": "obsidian-sample-plugin",
  "version": "0.9.7",
  "description": "This is a sample plugin for Obsidian (https://obsidian.md)",
  "main": "main.js",
  "scripts": {
    "dev": "rollup --config rollup/rollup.config.js -w",
    "doc": "typedoc Iris/source/main.ts --out docs/typedoc/",
    "build": "rollup --config rollup/rollup.config.js",
    "build-prod": "rollup --config rollup/rollup.config.js && cp obsidian/*.* build/",
    "test": "env TS_NODE_COMPILER_OPTIONS='{\"module\": \"commonjs\" }' mocha -r ts-node/register 'tests/**/*.ts'"
  },
  "keywords": [],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@rollup/plugin-commonjs": "^15.1.0",
    "@rollup/plugin-node-resolve": "^9.0.0",
    "@rollup/plugin-typescript": "^6.0.0",
    "@types/chai": "^4.2.14",
    "@types/mocha": "^8.2.0",
    "@types/node": "^14.14.2",
    "chai": "^4.2.0",
    "mocha": "^8.2.1",
    "obsidian": "https://github.com/obsidianmd/obsidian-api/tarball/master",
    "rollup": "^2.32.1",
    "ts-node": "^9.1.1",
    "tslib": "^2.0.3",
    "typedoc": "^0.20.13",
    "typescript": "^4.0.3"
  }
}
